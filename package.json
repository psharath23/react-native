{
  "name": "sampleApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf src/*.js && rimraf src/**/*.js && rimraf *.js",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run tsc --",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "test": "jest"
  },
  "dependencies": {
    "@types/react": "^16.0.35",
    "lodash": "^4.17.4",
    "react": "16.0.0",
    "react-native": "0.51.0",
    "react-native-fs": "^2.9.6",
    "react-promise": "^2.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.5.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.6",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^9.4.0",
    "babel-jest": "22.0.4",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.0.4",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}